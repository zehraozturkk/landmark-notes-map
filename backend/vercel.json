{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/landmarks", "dest": "/index.js" },
      { "src": "/landmarks/(.*)", "dest": "/index.js" },
      { "src": "/user/login", "dest": "/index.js" },
      { "src": "/user/register", "dest": "/index.js" },
      { "src": "/visited_landmarks", "dest": "/index.js" },
      { "src": "/visited_landmarks/(.*)", "dest": "/index.js" },
      { "src": "/create_plan", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }