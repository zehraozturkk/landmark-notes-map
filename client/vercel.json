{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/user/index.html" },
      { "src": "/client", "dest": "/index.html" },
      { "src": "/(.+)", "dest": "/$1" }
    ]
  }